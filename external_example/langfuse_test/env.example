# Langfuse Configuration
# Copy this file to .env and update with your actual credentials

# Required: Your Langfuse public key
LANGFUSE_PUBLIC_KEY=your_public_key_here

# Required: Your Langfuse secret key  
LANGFUSE_SECRET_KEY=your_secret_key_here

# Optional: Langfuse host (defaults to https://cloud.langfuse.com)
LANGFUSE_HOST=https://cloud.langfuse.com

# Optional: Enable debug mode for verbose logging
LANGFUSE_DEBUG=true
